	</xs:complexType>
	<xs:complexType name="RedirectResponseType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="RedirectUrl" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="UrlParams" type="xspg:KeyValueType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Yes / No / Unknown -->
	<xs:simpleType name="YNU">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Y"/>
			<xs:enumeration value="N"/>
			<xs:enumeration value="U"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="YNUA">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Y"/>
			<xs:enumeration value="N"/>
			<xs:enumeration value="U"/>
			<xs:enumeration value="A"/>
		</xs:restriction>
	</xs:simpleType>
	
	<!-- 3D Secure Object -->
	<xs:complexType name="ThreeDSecureType">
		<xs:sequence>
			<xs:element name="Enrolled" type="xspg:YNU"/>
