<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:sch="http://www.paygate.co.za/PayHOST" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.paygate.co.za/PayHOST" targetNamespace="http://www.paygate.co.za/PayHOST">
  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xspg="http://www.paygate.co.za/PayHOST" elementFormDefault="qualified" targetNamespace="http://www.paygate.co.za/PayHOST">

	<!-- Account Type -->
	<xs:complexType name="PayGateAccountType">
		<xs:sequence>
			<xs:element name="PayGateId" type="xs:token"/>
			<xs:element name="Password" type="xs:token"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Payment Method Type -->
	<xs:simpleType name="PaymentMethodType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="CC">
				<xs:annotation>
					<xs:documentation>
						<p>Credit Card</p>
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DC">
				<xs:annotation>
					<xs:documentation>
						<p>Debit Card</p>
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EW">
				<xs:annotation>
					<xs:documentation>
						<p>eWallet</p>
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BT">
				<xs:annotation>
					<xs:documentation>
						<p>Bank Transfer</p>
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CV">
				<xs:annotation>
					<xs:documentation>
						<p>Cash Voucher</p>
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PC">
				<xs:annotation>
					<xs:documentation>
						<p>Pre-Paid Card</p>
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<!-- PaymentMethodDetailType -->
	<xs:simpleType name="PaymentDetailType">
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="80"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- PaymentType -->
	<xs:complexType name="PaymentType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="Method" type="xspg:PaymentMethodType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Detail" type="xspg:PaymentDetailType"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Transaction Type -->
	<xs:simpleType name="TransactionType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Authorisation"/>
			<xs:enumeration value="Settlement"/>
			<xs:enumeration value="Refund"/>
			<xs:enumeration value="Payout"/>
			<xs:enumeration value="Purchase"/>	
		</xs:restriction>
	</xs:simpleType>
	
	<!-- Transaction Types including the all option -->
	<xs:simpleType name="QueryTransactionType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Authorisation"/>
			<xs:enumeration value="Settlement"/>
			<xs:enumeration value="Refund"/>	
			<xs:enumeration value="Payout"/>
			<xs:enumeration value="Purchase"/>	
			<xs:enumeration value="All"/>		
		</xs:restriction>
	</xs:simpleType>
	
	<!-- Risk Type -->
	<xs:complexType name="RiskType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="AccountNumber" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="SessionId" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="IpV4Address" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="IpV6Address" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="UserId" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="MachineId" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="UserProfile" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ConsumerWatch" type="xs:boolean"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Browser" type="xspg:BrowserType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Person Type -->
	<xs:complexType name="PersonType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="Title" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="1" name="FirstName" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="MiddleName" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="1" name="LastName" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Telephone" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Mobile" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Fax" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="1" name="Email" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="DateOfBirth" type="xs:date"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Nationality" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="IdNumber" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="IdType" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="SocialSecurityNumber" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Address" type="xspg:AddressType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BrowserType">
		<xs:sequence>
			<xs:element name="UserAgent" type="xs:string"/>
			<xs:element name="Language" type="xspg:LanguageType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Address Type -->
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="AddressLine" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="City" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Country" type="xspg:CountryType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="State" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Zip" type="xs:token"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Shipping Details Type -->
	<xs:complexType name="ShippingDetailsType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="Customer" type="xspg:PersonType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Address" type="xspg:AddressType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="DeliveryDate" type="xs:dateTime"/>
			<xs:element maxOccurs="1" minOccurs="0" name="DeliveryMethod" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="InstallationRequested" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Billing Details Type -->
	<xs:complexType name="BillingDetailsType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="Customer" type="xspg:PersonType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Address" type="xspg:AddressType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Order Item Type -->
	<xs:complexType name="OrderItemType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="ProductCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ProductDescription" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ProductCategory" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ProductRisk" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="OrderQuantity" type="xs:unsignedInt"/>
			<xs:element maxOccurs="1" minOccurs="0" name="UnitPrice" type="xs:decimal"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Currency" type="xs:token"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Order Type -->
	<xs:complexType name="OrderType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="MerchantOrderId" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Currency" type="xspg:CurrencyType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Amount" type="xs:int"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Discount" type="xs:int"/>
			<xs:element maxOccurs="1" minOccurs="0" name="TransactionDate" type="xs:dateTime"/>
			<xs:element maxOccurs="1" minOccurs="0" name="BillingDetails" type="xspg:BillingDetailsType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ShippingDetails" type="xspg:ShippingDetailsType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="AirlineBookingDetails" type="xspg:AirlineBookingDetailsType"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="OrderItems" type="xspg:OrderItemType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Locale" type="xspg:LanguageType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Passenger Type -->
	<xs:complexType name="PassengerType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="Passenger" type="xspg:PersonType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="TravellerType">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="A"/>
						<xs:enumeration value="C"/>
						<xs:enumeration value="T"/>
						<xs:enumeration value="I"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="LoyaltyNumber" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="LoyaltyType" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="LoyaltyTier" type="xs:int"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="CarrierCodeType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[A-Z0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Flight Leg Type -->
	<xs:complexType name="FlightLegType">
		<xs:sequence>		
		
			<!-- Departure Details -->
			<xs:element maxOccurs="1" minOccurs="1" name="DepartureAirport">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="[A-Z]{3}"/>
					</xs:restriction>				
				</xs:simpleType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="1" name="DepartureCountry" type="xspg:CountryType"/>			
			<xs:element maxOccurs="1" minOccurs="1" name="DepartureCity">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="[A-Z]{3}"/>
					</xs:restriction>				
				</xs:simpleType>			
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="1" name="DepartureDateTime" type="xs:dateTime"/>
			<xs:element maxOccurs="1" minOccurs="0" name="DepartureAirportTimeZone" type="xspg:TimeZoneType"/>

			<!-- Arrival Details -->
			<xs:element maxOccurs="1" minOccurs="1" name="ArrivalAirport">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="[A-Z]{3}"/>					
					</xs:restriction>				
				</xs:simpleType>
			</xs:element>			
			<xs:element maxOccurs="1" minOccurs="1" name="ArrivalCountry" type="xspg:CountryType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="ArrivalCity">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="[A-Z]{3}"/>
					</xs:restriction>				
				</xs:simpleType>			
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="1" name="ArrivalDateTime" type="xs:dateTime"/>			
			<xs:element maxOccurs="1" minOccurs="0" name="ArrivalAirportTimeZone" type="xspg:TimeZoneType"/>

			<!-- Carrier Details -->
			<xs:element maxOccurs="1" minOccurs="1" name="MarketingCarrierCode" type="xspg:CarrierCodeType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="MarketingCarrierName" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="1" name="IssuingCarrierCode" type="xspg:CarrierCodeType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="IssuingCarrierName" type="xs:token"/>			
			
			<!-- Flight Details -->
			<xs:element maxOccurs="1" minOccurs="1" name="FlightNumber" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="FareBasisCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="FareClass" type="xs:token"/>
			
			<!-- Money Details -->
			<xs:element maxOccurs="1" minOccurs="1" name="BaseFareAmount" type="xs:int"/>
			<xs:element maxOccurs="1" minOccurs="1" name="BaseFareCurrency" type="xspg:CurrencyType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Airline Booking Type -->
	<xs:complexType name="AirlineBookingDetailsType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="TicketNumber" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="InternalCustomerCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ReservationSystem" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="TravelAgencyCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="TravelAgencyName" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="PayerTravelling" type="xs:boolean"/>
			<xs:element maxOccurs="1" minOccurs="1" name="PNR" type="xs:token"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Passengers" type="xspg:PassengerType"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="FlightLegs" type="xspg:FlightLegType"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Language Type according to RFC 1766 -->
	<xs:simpleType name="LanguageType">
		<xs:restriction base="xs:token">
			<xs:pattern value="([a-zA-Z]{2}|[iI]-[a-zA-Z]+|[xX]-[a-zA-Z]{1,8})(-[a-zA-Z]{1,8})*"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Credit Card Type -->
	<xs:simpleType name="CreditCardType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Unknown"/>
			<xs:enumeration value="Visa"/>
			<xs:enumeration value="MasterCard"/>
			<xs:enumeration value="AmericanExpress"/>
			<xs:enumeration value="DinersClub"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- TransactionCode -->
	<xs:simpleType name="TransactionCodeType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="NotDone"/>
			<xs:enumeration value="Approved"/>
			<xs:enumeration value="Declined"/>
			<xs:enumeration value="Paid"/>
			<xs:enumeration value="Refunded"/>
			<xs:enumeration value="ReceivedByPaygate"/>
			<xs:enumeration value="RepliedToClient"/>
		</xs:restriction>
	</xs:simpleType>
	
	<!-- Vault Key Type -->
	<xs:simpleType name="VaultKeyType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="cardNumber"/>
			<xs:enumeration value="expDate"/>
		</xs:restriction>
	</xs:simpleType>

