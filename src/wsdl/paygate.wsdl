	<!-- Credit Card Payment Request -->
	<xs:complexType name="CardPaymentRequestType">
		<xs:sequence>
			<xs:element name="Account" type="xspg:PayGateAccountType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Customer" type="xspg:PersonType"/>
			<xs:choice>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="CardNumber" type="xspg:CardNumberType"/>
					<xs:element name="CardExpiryDate" type="xspg:CardDateType"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="VaultId" type="xs:token"/>
				</xs:sequence>
			</xs:choice>
			<xs:element maxOccurs="1" minOccurs="0" name="CardIssueDate" type="xspg:CardDateType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="CardIssueNumber" type="xspg:IssueNumberType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="CVV" type="xspg:CvvType"/>
			<xs:element default="false" maxOccurs="1" minOccurs="0" name="Vault" type="xs:boolean"/>
			<xs:element maxOccurs="1" minOccurs="0" name="BudgetPeriod" type="xspg:BudgetPeriodType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Redirect" type="xspg:RedirectRequestType"/>
			<xs:element name="Order" type="xspg:OrderType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ThreeDSecure" type="xspg:ThreeDSecureType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Risk" type="xspg:RiskType"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="UserDefinedFields" type="xspg:KeyValueType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="BillingDescriptor" type="xs:token"/>
		</xs:sequence>
