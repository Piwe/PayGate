	<!-- Timezone Type -->
	<xs:simpleType name="TimeZoneType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[+-][0-9]{2}:[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Budget Period Type -->
	<xs:simpleType name="BudgetPeriodType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[0-9]+"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- CVV Type -->
	<xs:simpleType name="CvvType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[0-9]+"/>
			<xs:minLength value="3"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Issue Number Type -->
	<xs:simpleType name="IssueNumberType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[0-9]+"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Card Expiry / Issue Date Type -->
	<xs:simpleType name="CardDateType">
		<xs:restriction base="xs:token">
			<xs:pattern value="([0][0-9]|[1][0-2])20[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Status Name Type -->
	<xs:simpleType name="StatusNameType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Error"/>
			<xs:enumeration value="Pending"/>
			<xs:enumeration value="Cancelled"/>
			<xs:enumeration value="Completed"/>
			<xs:enumeration value="RiskRejected"/>
			<xs:enumeration value="ValidationError"/>
			<xs:enumeration value="WebRedirectRequired"/>
			<xs:enumeration value="ThreeDSecureRedirectRequired"/>
			<xs:enumeration value="VaultFailure"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Card Number Type -->
	<xs:simpleType name="CardNumberType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[0-9x]+"/>
			<xs:minLength value="13"/>
			<xs:maxLength value="19"/>
		</xs:restriction>
	</xs:simpleType>
	
	<!-- Status Type -->
	<xs:complexType name="StatusType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="TransactionId" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Reference" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="0" name="AcquirerCode" type="xs:string"/>

			<!-- Used for if errors occur in code -->
			<xs:element maxOccurs="1" minOccurs="0" name="StatusName" type="xspg:StatusNameType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="StatusDetail" type="xs:token"/>

			<xs:element maxOccurs="1" minOccurs="0" name="AuthCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="PayRequestId" type="xs:token"/>
				
			<xs:element maxOccurs="1" minOccurs="0" name="VaultId" type="xs:token"/>
			
			<xs:element maxOccurs="unbounded" minOccurs="0" name="PayVaultData" type="xspg:VaultDataType"/>
			
			<xs:element maxOccurs="1" minOccurs="0" name="TransactionStatusCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="TransactionStatusDescription" type="xs:token"/>
				
			<xs:element maxOccurs="1" minOccurs="0" name="ResultCode" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ResultDescription" type="xs:token"/>

			<xs:element maxOccurs="1" minOccurs="0" name="Currency" type="xspg:CurrencyType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="Amount" type="xs:int"/>
				
			<xs:element maxOccurs="1" minOccurs="0" name="RequestedCurrency" type="xspg:CurrencyType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="RequestedAmount" type="xs:int"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ConversionRate" type="xs:token"/>
				
			<xs:element maxOccurs="1" minOccurs="0" name="RiskIndicator" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="PaymentType" type="xspg:PaymentType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="BillingDescriptor" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="0" name="DateTime" type="xs:dateTime"/>
			<xs:element maxOccurs="1" minOccurs="0" name="TransactionType" type="xspg:TransactionType"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Key Value Pair -->
	<xs:complexType name="KeyValueType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="key" type="xs:token"/>
			<xs:element maxOccurs="1" minOccurs="1" name="value" type="xs:token"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Vault data -->
	<xs:complexType name="VaultDataType">
