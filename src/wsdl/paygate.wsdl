	<!-- Redirect Object -->
	<xs:complexType name="RedirectRequestType">
		<xs:sequence>
			<xs:element name="NotifyUrl" type="xs:token"/>
			<xs:element name="ReturnUrl" type="xs:token"/>
			<xs:element default="_self" maxOccurs="1" minOccurs="0" name="Target" type="xs:token"/>
